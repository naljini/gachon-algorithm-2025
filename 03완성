        "import time\n",
        "\n",
        "def measure_real_performance():\n",
        "    \"\"\"ì‹¤ì œ ë°ì´í„°ë¡œ ì„±ëŠ¥ ì¸¡ì •\"\"\"\n",
        "    sizes = [1000, 5000, 10000]\n",
        "\n",
        "    for size in sizes:\n",
        "        data = list(range(size))\n",
        "\n",
        "        # ìµœì„ ì˜ ê²½ìš°\n",
        "        start = time.time()\n",
        "        result = data[0] in data  # ì²« ë²ˆì§¸ ì›ì†Œ ì°¾ê¸°\n",
        "        best_time = time.time() - start\n",
        "\n",
        "        # ìµœì•…ì˜ ê²½ìš°\n",
        "        start = time.time()\n",
        "        result = data[-1] in data  # ë§ˆì§€ë§‰ ì›ì†Œ ì°¾ê¸°\n",
        "        worst_time = time.time() - start\n",
        "\n",
        "        print(f\"í¬ê¸° {size:>6}: ìµœì„  {best_time:.6f}ì´ˆ, ìµœì•… {worst_time:.6f}ì´ˆ\")\n",
