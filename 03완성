        "def quadratic_operation(n):\n",
        "    count = 0\n",
        "    for i in range(n):\n",
        "        for j in range(n):  # n * në²ˆ ë°˜ë³µ\n",
        "            count += i + j\n",
        "    return count\n",
        "\n",
