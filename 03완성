      "source": [
        "# 2.ì¬ê·€ ì•Œê³ ë¦¬ì¦˜ ì‚¬ìš©\n",
        "def palindrome(tStr):         # ì¬ê·€ë°©ë²•ìœ¼ë¡œ íšŒë¬¸ ì—¬ë¶€ ì²´í¬\n",
        "    if len(tStr) <= 1:                      # Base case --> ë¬¸ìì—´ ê¸¸ì´ê°€ 1ì´í•˜ë©´\n",
        "        return True\n",
        "    if tStr[0] != tStr[-1]:                 # ì²« ê¸€ì&ë ê¸€ìê°€ ë™ì¼í•œì§€ ì²´í¬\n",
        "        return False\n",
        "    return palindrome(tStr[1:len(tStr)-1])  # ì¬ê·€ í˜¸ì¶œ\n",
        "\n",
        "\n",
        "def is_palindrome(tStrings):  # íšŒë¬¸ ì—¬ë¶€ íŒë³„\n",
        "    for tStr in tStrings:                    # ë¬¸ìì—´ í•˜ë‚˜ì”© ì½ê¸°\n",
        "        tStr = tStr.lower().replace(' ','')  # ì†Œë¬¸ì->ê³µë°±ì œê±°\n",
        "        if palindrome(tStr):                 # íŒë³„ í•¨ìˆ˜ í˜¸ì¶œ\n",
        "            print(f\"{tStr} --> True\")\n",
        "        else:\n",
        "            print(f\"{tStr} --> False\")\n",
        "\n",
        "is_palindrome(['reaver', 'level','ê¸°ëŸ¬ê¸°','ì‚´ê¸ˆ ì‚´ê¸ˆ'])"
      ]
    },
