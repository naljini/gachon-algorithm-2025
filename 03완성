        "# 1.append() ë©”ì„œë“œ\n",
        "def test1(n):\n",
        "    l = []\n",
        "    for i in range(n):\n",
        "        l.append(i)\n",
        "\n",
        "# 2.extend() ë©”ì„œë“œ\n",
        "def test2(n):\n",
        "    l = []\n",
        "    for i in range(n):\n",
        "        l.extend([i])\n",
        "\n",
        "# 3.ë¦¬ìŠ¤íŠ¸ ì—°ê²° ì—°ì‚°ì\n",
        "def test3(n):\n",
        "    l = []\n",
        "    for i in range(n):\n",
        "        l = l + [i]\n",
        "\n",
        "# 4.ë¦¬ìŠ¤íŠ¸ ì¡°ê±´ì œì‹œë²•\n",
        "def test4(n):\n",
        "    l = [i for i in range(n)]\n",
        "\n",
        "# 5.range ê°ì²´ í™œìš©\n",
        "def test5(n):\n",
        "    l = list(range(n))"
      ],
