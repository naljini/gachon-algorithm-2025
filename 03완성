    {
      "cell_type": "code",
      "source": [
        "import time\n",
        "import random\n",
        "from functools import lru_cache\n",
        "\n",
        "# ==============================================================\n",
        "# 1. ì¬ê·€ í•¨ìˆ˜ (ìˆœìˆ˜í•œ ìˆ˜í•™ì  ì •ì˜ì™€ ì¼ì¹˜)\n",
        "# ==============================================================\n",
        "def recursive_fibonacci(n):\n",
        "    \"\"\"ì¬ê·€ í•¨ìˆ˜ë¡œ êµ¬í˜„í•œ í”¼ë³´ë‚˜ì¹˜\"\"\"\n",
        "    if n <= 1:\n",
        "        return n\n",
        "    return recursive_fibonacci(n-1) + recursive_fibonacci(n-2)\n",
        "\n",
        "def recursive_factorial(n):\n",
        "    \"\"\"ì¬ê·€ í•¨ìˆ˜ë¡œ êµ¬í˜„í•œ íŒ©í† ë¦¬ì–¼\"\"\"\n",
        "    if n <= 1:\n",
        "        return 1\n",
        "    return n * recursive_factorial(n-1)\n",
        "\n",
        "# ==============================================================\n",
        "# 2. ë©”ëª¨ì´ì œì´ì…˜ (ì¤‘ë³µ ê³„ì‚° ë°©ì§€, ì¬ê·€ ìœ ì§€)\n",
        "# ==============================================================\n",
        "@lru_cache(maxsize=None)   # LRU = Least Recently Used (ê°€ì¥ ì˜¤ë˜ ì‚¬ìš©í•˜ì§€ ì•Šì€ ê²ƒ ë¨¼ì € ë²„ë¦¼) í•´ì„œ ì¤‘ë³µ ê³„ì‚°ì„ í”¼í•˜ë„ë¡ ë„ì™€\n",
        "def memoized_fibonacci(n):\n",
        "    \"\"\"ë©”ëª¨ì´ì œì´ì…˜ì„ ì‚¬ìš©í•œ í”¼ë³´ë‚˜ì¹˜\"\"\"\n",
        "    if n <= 1:\n",
        "        return n\n",
        "    return memoized_fibonacci(n-1) + memoized_fibonacci(n-2)\n",
        "\n",
        "# ìˆ˜ë™ ë©”ëª¨ì´ì œì´ì…˜ ì˜ˆì œ\n",
        "memo_dict = {}\n",
        "def manual_memoized_factorial(n):\n",
        "    \"\"\"ìˆ˜ë™ ë©”ëª¨ì´ì œì´ì…˜ì„ ì‚¬ìš©í•œ íŒ©í† ë¦¬ì–¼\"\"\"\n",
        "    if n in memo_dict:\n",
        "        return memo_dict[n]\n",
        "\n",
        "    if n <= 1:\n",
        "        result = 1\n",
        "    else:\n",
        "        result = n * manual_memoized_factorial(n-1)\n",
        "\n",
        "    memo_dict[n] = result\n",
        "    return result\n",
        "\n",
        "# ==============================================================\n",
        "# 3. ë°˜ë³µë¬¸ (Bottom-up) - ë©”ëª¨ë¦¬ íš¨ìœ¨ì„±, ë¹ ë¥¸ ì‹¤í–‰\n",
        "# ==============================================================\n",
        "def iterative_fibonacci(n):\n",
        "    \"\"\"ë°˜ë³µë¬¸ìœ¼ë¡œ êµ¬í˜„í•œ í”¼ë³´ë‚˜ì¹˜\"\"\"\n",
        "    if n <= 1:\n",
        "        return n\n",
        "\n",
        "    # ê³µê°„ ìµœì í™” ë²„ì „ (O(1) ê³µê°„)\n",
        "    prev2, prev1 = 0, 1\n",
        "    for i in range(2, n + 1):\n",
        "        current = prev1 + prev2\n",
        "        prev2, prev1 = prev1, current\n",
        "\n",
        "    return prev1\n",
        "\n",
        "def iterative_factorial(n):\n",
        "    \"\"\"ë°˜ë³µë¬¸ìœ¼ë¡œ êµ¬í˜„í•œ íŒ©í† ë¦¬ì–¼\"\"\"\n",
        "    result = 1\n",
        "    for i in range(1, n + 1):\n",
        "        result *= i\n",
        "    return result\n",
        "\n",
        "def iterative_sum_sequence(n):\n",
        "    \"\"\"ë°˜ë³µë¬¸ìœ¼ë¡œ êµ¬í˜„í•œ í•© ìˆ˜ì—´: S(n) = 1 + 2 + ... + n\"\"\"\n",
        "    total = 0\n",
        "    for i in range(1, n + 1):\n",
        "        total += i\n",
        "    return total\n",
        "\n",
        "# ==============================================================\n",
        "# 4. í–‰ë ¬ ê±°ë“­ì œê³± (ë§¤ìš° í° nì— ëŒ€í•´ íš¨ìœ¨ì )\n",
        "# ==============================================================\n",
        "def matrix_multiply_2x2(A, B):\n",
        "    \"\"\"2x2 í–‰ë ¬ ê³±ì…ˆ\"\"\"\n",
        "    return [\n",
        "        [A[0][0]*B[0][0] + A[0][1]*B[1][0], A[0][0]*B[0][1] + A[0][1]*B[1][1]],\n",
        "        [A[1][0]*B[0][0] + A[1][1]*B[1][0], A[1][0]*B[0][1] + A[1][1]*B[1][1]]\n",
        "    ]\n",
        "\n",
        "def matrix_power(matrix, n):\n",
        "    \"\"\"í–‰ë ¬ì˜ nì œê³± (ê³ ì† ê±°ë“­ì œê³± ì‚¬ìš©)\"\"\"\n",
        "    if n == 1:\n",
        "        return matrix\n",
        "\n",
        "    if n % 2 == 0:\n",
        "        half = matrix_power(matrix, n // 2)\n",
        "        return matrix_multiply_2x2(half, half)\n",
        "    else:\n",
        "        return matrix_multiply_2x2(matrix, matrix_power(matrix, n - 1))\n",
        "\n",
        "def matrix_fibonacci(n):\n",
        "    \"\"\"í–‰ë ¬ ê±°ë“­ì œê³±ìœ¼ë¡œ êµ¬í˜„í•œ í”¼ë³´ë‚˜ì¹˜\"\"\"\n",
        "    if n <= 1:\n",
        "        return n\n",
        "\n",
        "    # í”¼ë³´ë‚˜ì¹˜ ê¸°ë³¸ í–‰ë ¬: [[1, 1], [1, 0]]\n",
        "    base_matrix = [[1, 1], [1, 0]]\n",
        "    result_matrix = matrix_power(base_matrix, n)\n",
        "\n",
        "    return result_matrix[0][1]  # F(n)\n",
        "\n",
        "# ==============================================================\n",
        "# í…ŒìŠ¤íŠ¸ ë° ì„±ëŠ¥ ì¸¡ì • í•¨ìˆ˜ë“¤\n",
        "# ==============================================================\n",
        "def measure_execution_time(func, n, func_name):\n",
        "    \"\"\"í•¨ìˆ˜ ì‹¤í–‰ ì‹œê°„ ì¸¡ì •\"\"\"\n",
        "    start_time = time.time()\n",
        "    try:\n",
        "        result = func(n)\n",
        "        end_time = time.time()\n",
        "        execution_time = end_time - start_time\n",
        "        return result, execution_time, \"ì„±ê³µ\"\n",
        "    except RecursionError:\n",
        "        return None, None, \"ì¬ê·€ ê¹Šì´ ì´ˆê³¼\"\n",
        "    except Exception as e:\n",
        "        return None, None, f\"ì˜¤ë¥˜: {str(e)}\"\n",
        "\n",
        "def generate_test_data():\n",
        "    \"\"\"í…ŒìŠ¤íŠ¸ìš© ì„ì˜ ë°ì´í„° ìƒì„±\"\"\"\n",
        "    # ë‹¤ì–‘í•œ í¬ê¸°ì˜ í…ŒìŠ¤íŠ¸ ë°ì´í„°\n",
        "    small_values = [1, 2, 3, 5, 8]  # ì‘ì€ ê°’ë“¤\n",
        "    medium_values = [10, 15, 20, 25]  # ì¤‘ê°„ ê°’ë“¤\n",
        "    large_values = [30, 35, 40]  # í° ê°’ë“¤ (ì¬ê·€ì—ëŠ” ìœ„í—˜)\n",
        "\n",
        "    # ëœë¤í•˜ê²Œ ëª‡ ê°œ ì„ íƒ\n",
        "    test_cases = []\n",
        "    test_cases.extend(random.sample(small_values, 3))\n",
        "    test_cases.extend(random.sample(medium_values, 2))\n",
        "    test_cases.append(random.choice(large_values))\n",
        "\n",
        "    return sorted(test_cases)\n",
        "\n",
        "def test_fibonacci_methods():\n",
        "    \"\"\"í”¼ë³´ë‚˜ì¹˜ êµ¬í˜„ ë°©ë²•ë“¤ í…ŒìŠ¤íŠ¸\"\"\"\n",
        "    print(\"=\" * 70)\n",
        "    print(\"ğŸ”¢ í”¼ë³´ë‚˜ì¹˜ ìˆ˜ì—´ ê³„ì‚° - 4ê°€ì§€ ë°©ë²• ë¹„êµ\")\n",
        "    print(\"=\" * 70)\n",
        "\n",
        "    # í…ŒìŠ¤íŠ¸ ë°ì´í„° ìƒì„±\n",
        "    test_values = generate_test_data()\n",
        "    print(f\"ğŸ“Š í…ŒìŠ¤íŠ¸ ê°’ë“¤: {test_values}\")\n",
        "    print()\n",
        "\n",
        "    # ê° ë°©ë²•ë³„ë¡œ í…ŒìŠ¤íŠ¸\n",
        "    methods = [\n",
        "        (\"ì¬ê·€ í•¨ìˆ˜\", recursive_fibonacci),\n",
        "        (\"ë©”ëª¨ì´ì œì´ì…˜\", memoized_fibonacci),\n",
        "        (\"ë°˜ë³µë¬¸\", iterative_fibonacci),\n",
        "        (\"í–‰ë ¬ ê±°ë“­ì œê³±\", matrix_fibonacci)\n",
        "    ]\n",
        "\n",
        "    print(f\"{'n':>3} | {'ë°©ë²•':^15} | {'ê²°ê³¼':>10} | {'ì‹œê°„(ì´ˆ)':>10} | {'ìƒíƒœ'}\")\n",
        "    print(\"-\" * 60)\n",
        "\n",
        "    for n in test_values:\n",
        "        for method_name, method_func in methods:\n",
        "            # ì¬ê·€ëŠ” í° ê°’ì—ì„œ ë„ˆë¬´ ì˜¤ë˜ ê±¸ë¦¬ë¯€ë¡œ ì œí•œ\n",
        "            if method_name == \"ì¬ê·€ í•¨ìˆ˜\" and n > 30:\n",
        "                print(f\"{n:>3} | {method_name:^15} | {'ê±´ë„ˆë›°ê¸°':>10} | {'-':>10} | ì‹œê°„ ì ˆì•½\")\n",
        "                continue\n",
        "\n",
        "            result, exec_time, status = measure_execution_time(method_func, n, method_name)\n",
        "\n",
        "            if status == \"ì„±ê³µ\":\n",
        "                print(f\"{n:>3} | {method_name:^15} | {result:>10} | {exec_time:>10.6f} | {status}\")\n",
        "            else:\n",
        "                print(f\"{n:>3} | {method_name:^15} | {'-':>10} | {'-':>10} | {status}\")\n",
        "\n",
        "        print(\"-\" * 60)\n",
        "\n",
        "\n",
        "# í…ŒìŠ¤íŠ¸\n",
        "test_fibonacci_methods()\n"
